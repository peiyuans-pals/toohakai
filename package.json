{
  "private": true,
  "engines": {
    "node": ">=18.15.0"
  },
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "turbo dev --no-cache  --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "lint:fix": "turbo lint --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "test": "turbo test",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.6.10"
}
